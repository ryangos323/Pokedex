<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: Blue;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pMain.MainView.MainViewControl">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <VBox alignment="TOP_CENTER" prefHeight="150.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Circle radius="70.0" stroke="BLACK" strokeType="INSIDE">
               <fill>
                  <LinearGradient endX="0.009523809523809525" endY="1.0" proportional="false">
                     <stops>
                        <Stop color="RED" />
                        <Stop color="RED" offset="0.007662835249042145" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </fill>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Circle>
            <Text fill="#00ff21" strokeType="OUTSIDE" strokeWidth="0.0" text="PokeDextrous" underline="true">
               <font>
                  <Font name="Bookshelf Symbol 7" size="57.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Text>
         </children>
      </VBox>
   </top>
   <center>
      <Button mnemonicParsing="false" onAction="#goPokedex" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: BLACK; -fx-border-color: White;" text="PokeDex" textFill="RED" BorderPane.alignment="CENTER">
         <font>
            <Font name="Bookshelf Symbol 7" size="26.0" />
         </font>
      </Button>
   </center>
   <bottom>
      <Button mnemonicParsing="false" onAction="#goCustoms" prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: black; -fx-border-color: white;" text="Customs" textFill="WHITE" textOverrun="CLIP" BorderPane.alignment="CENTER">
         <font>
            <Font name="Bookshelf Symbol 7" size="26.0" />
         </font>
      </Button>
   </bottom>
</BorderPane>
